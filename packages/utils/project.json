{
  "name": "utils",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/utils/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "test": {
      "inputs": ["default", "^production"],
      "outputs": ["!{projectRoot}/coverage"],
      "options": {
        "passWithNoTests": true,
        "coverage": true,
        "coverageDirectory": "coverage",
        "coverageThreshold": {
          "global": {
            "branches": 70,
            "functions": 70,
            "lines": 70,
            "statements": 70
          }
        }
      }
    }
  }
}
