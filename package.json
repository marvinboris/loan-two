  "scripts": {
    "test": "nx run-many -t test --coverage",
    "test:watch": "nx run-many -t test --watch",
    "test:affected": "nx affected -t test --coverage",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "nx run-many -t typecheck",
    "prepare": "husky install",
    "seed": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/index.ts",
    "seed:users": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/index.ts users",
    "seed:customers": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/index.ts customers",
    "seed:loans": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/index.ts loans",
    "seed:collection-records": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/index.ts collection-records",
    "seed:performances": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/index.ts performances",
    "seed:repayments": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/index.ts repayments",
    "seed:all": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/index.ts all",
    "seed:user": "dotenv -e apps/api/.env -- tsx apps/api/src/seeders/user.ts",
    "prepare-api": "nx build api && rm -rf ~/cfafrica ~/cfafrica.zip && cp -r dist/api . && mv api cfafrica && zip -r cfafrica.zip cfafrica && mv cfafrica.zip ~ && mv cfafrica ~",
    "deploy-admin": "nx build admin && cd apps/admin && firebase deploy && cd ../..",
    "build-customer": "cd apps/customer && pnpm android-prev && cd ../..",
    "build-telemarketing": "cd apps/telemarketing && pnpm android-prev && cd ../..",
    "build-collection": "cd apps/collection && pnpm android-prev && cd ../..",
    "test:pixpay": "dotenv -e apps/api/.env -- tsx apps/api/src/scripts/test-pixpay.ts",
    "test:iwomi": "dotenv -e apps/api/.env -- tsx apps/api/src/scripts/test-iwomi.ts"
  },
